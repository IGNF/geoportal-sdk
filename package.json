    "gulp-mocha-phantomjs": "^0.7.0",
    "gulp-notify": "^1.2.1",
    "gulp-plumber": "~0.5.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-size": "~0.1.2",
    "gulp-template": "^3.1.0",
    "gulp-uglify": "~0.2.1",
    "gulp-umd": "^0.2.0",
    "gulp-useref": "^1.2.0",
    "gulp-util": "~2.2.14",
    "gulp-watch": "~0.5.3",
