{
    "name": "geoportal-sdk",
    "version": "2.0.2",
    "SDK2DVersion": "2.0.2",
    "SDK3DVersion": "2.0.2",
    "description": "French Geoportal SDK based on OpenLayers (2D) and iTowns (3D) libraries",
    "main": "dist/2d/GpSDK2D.js, dist/3d/GpSDK3d.js",
    "scripts": {
        "setup": "npm install",
        "clean": "echo \"Warning: target not yet implemented!\" && exit 0",
        "test:serve": "webpack-dev-server --hot --config webpack.test.serve.js",
        "doc:serve": "webpack-dev-server --config webpack.config.3d --contentBase jsdoc/ --port 9001 --open",
        "sample:3d:serve": "webpack-dev-server --config webpack.config.3d --open-page samples/index-3d-src.html --contentBase . --port 9001 --open",
        "sample:2d:serve": "webpack-dev-server --config webpack.config.2d --open-page samples/index-2d-src.html --contentBase . --port 9001 --open",
        "stat:2d": "webpack --config webpack.config.2d --profile --json > map-2d.json",
        "stat:3d": "webpack --config webpack.config.3d --profile --json > map-3d.json",
        "build:dev": "npm run build:2d:dev; npm run build:3d:dev",
        "build:prod": "npm run build:2d:prod; npm run build:3d:prod",
        "build": "npm run build:2d; npm run build:3d",
        "build:3d": "webpack --config webpack.config.3d",
        "build:3d:dev": "webpack --config webpack.config.3d --env.development",
        "build:3d:prod": "webpack --config webpack.config.3d --env.production",
        "build:2d": "webpack --config webpack.config.2d",
        "build:2d:dev": "webpack --config webpack.config.2d --env.development",
        "build:2d:prod": "webpack --config webpack.config.2d --env.production"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/IGNF/geoportal-sdk.git"
    },
    "keywords": [
        "openlayers",
        "itowns",
        "geoportail",
        "webservice",
        "javascript"
    ],
    "author": "IGNF",
    "license": "CECILL-B",
    "bugs": {
        "url": "https://github.com/IGNF/geoportal-sdk/issues"
    },
    "homepage": "https://geoservices.ign.fr/documentation/utilisation_web/sdk.html",
    "dependencies": {
        "geoportal-extensions-openlayers": "2.1.2",
        "geoportal-extensions-openlayers-itowns": "2.1.2",
        "itowns": "2.3.0",
        "openlayers": "4.4.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "chai": "^4.1.2",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.11",
        "eslint": "^4.18.2",
        "eslint-config-standard": "^11.0.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "expose-loader": "^0.7.5",
        "extract-text-webpack-plugin": "^3.0.2",
        "handlebars": "^4.0.11",
        "handlebars-layouts": "^3.1.4",
        "html-webpack-plugin": "^3.2.0",
        "jsdoc-webpack-plugin": "0.0.1",
        "loglevel": "^1.6.1",
        "mocha": "^5.2.0",
        "mocha-loader": "^1.1.3",
        "mocha-webpack": "^1.1.0",
        "path": "^0.12.7",
        "proj4": "^2.4.4",
        "replace-bundle-webpack-plugin": "^1.0.0",
        "requirejs": "^2.3.5",
        "speed-measure-webpack-plugin": "^1.2.2",
        "string-template": "^1.0.0",
        "style-loader": "^0.20.2",
        "three": "^0.93.0",
        "three.meshline": "^1.1.0",
        "url-loader": "^1.0.1",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.1"
    }
}
